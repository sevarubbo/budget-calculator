{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": ["error"],
    "padded-blocks": ["error", "never"],
    "no-multiple-empty-lines": "error",
    "quotes": ["error", "single"]
  }
}
